{
  "name": "extension-ows",
  "description": "Extension Chrome/Firefox - Outils de dÃ©veloppement pour les templates OWS",
  "private": true,
  "version": "1.0.4",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "clean": "rm -rf .output/*.zip .output/*.crx",
    "clean:chrome": "rm -rf .output/*-chrome.zip .output/*.crx",
    "clean:firefox": "rm -rf .output/*-firefox.zip .output/*-sources.zip",
    "build": "npm run clean:chrome && wxt build",
    "build:firefox": "npm run clean:firefox && wxt build -b firefox",
    "zip": "npm run clean:chrome && wxt zip",
    "zip:firefox": "npm run clean:firefox && wxt zip -b firefox",
    "zip:project": "zip -r extension-ows.zip . -x 'node_modules/*' '.git/*' '.output/*' '*.zip' '.DS_Store' 'Archive.zip'",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare"
  },
  "keywords": ["chrome-extension", "firefox-addon", "developer-tools", "ows", "prisma-media"],
  "author": {
    "name": "Bobywan",
    "email": "simon.banchet@gmail.com"
  },
  "license": "ISC",
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.4",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.10.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "json-display": "^1.2.1",
    "mini-css-extract-plugin": "^2.8.1",
    "postcss": "^8.5.3",
    "postcss-advanced-variables": "^5.0.0",
    "postcss-custom-media": "^11.0.5",
    "postcss-custom-properties-transformer": "^1.0.0",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-nested": "^7.0.2",
    "postcss-preset-env": "^10.1.6",
    "postcss-url": "^10.1.3",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.14",
    "typescript": "^5.8.3",
    "vue": "^3.5.16",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "wxt": "^0.20.6"
  },
  "dependencies": {
    "@material/web": "^2.3.0"
  }
}
